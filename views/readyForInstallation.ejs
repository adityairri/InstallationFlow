                                                        var x = document.getElementById("SElistSelect<%=singleData.data.id%>").value;
                                                        url = "/assignSE/<%=singleData.data.id%>/" + x;
                                                        $("#assignSE<%=singleData.data.id%>").attr("href", url);
                                                        return true;
                                                    }

                                                    function selectedSE<%=singleData.data.id%> () {
                                                        var x = document.getElementById("SElistSelect<%=singleData.data.id%>").value;
                                                        url = "/assignSE/<%=singleData.data.id%>/" + x;
                                                        $("#assignSE<%=singleData.data.id%>").attr("href", url);
                                                        return true;
                                                    }
                                                </script>
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <!-- <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                                                        <a href="#" id="assignSE<%=singleData.data.id%>"><button

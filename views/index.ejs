                              var selectedTimeframe = localStorage.getItem("selectedTimeframe<%= singleData.data.id%>");
                              var remarks = localStorage.getItem("remarks<%= singleData.data.id%>");

                              url = "/assignDate/<%=singleData.data.id%>/" + selectedFarmId + "/" + selectedDate + "/" + selectedTimeframe + "/" + remarks;
